{
  "name": "my-plugin",
  "version": "1.0.0",
"scripts": {
    "setup": "node setup.js",
    "setup:cleanup": "del-cli setup.js",
    "setup:complete": "npm run setup && npm run setup:cleanup",
    
    "sass": "sass assets/src/css:assets/build/css --no-source-map",
    "sass:watch": "sass assets/src/css:assets/build/css --no-source-map --watch",
    "js:minify": "node scripts/minify-js.js",
    "watch:sass": "npm run sass:watch",
    "watch:js": "chokidar 'assets/src/js/*.js' -c 'npm run js:minify'",
    "watch": "npm-run-all --parallel watch:*",
    "build:sass": "npm run sass",
    "build:js": "npm run js:minify",
    "build": "npm-run-all build:*",
    "dev": "npm run watch"
  },
  "devDependencies": {
    "sass": "^1.70.0",
    "terser": "^5.27.0",
    "npm-run-all": "^4.1.5",
    "chokidar-cli": "^3.0.0"
  }
}